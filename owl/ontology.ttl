@prefix : <http://www.purl.org/graphemon#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cidoc: <http://www.cidoc-crm.org/cidoc-crm/> .
@prefix lemon: <http://lemon-model.net/lemon#> .
@base <http://www.purl.org/graphemon> .

<http://www.purl.org/graphemon> rdf:type owl:Ontology ;
                                 rdfs:label "Grapheme Model for Ontologies (Graphemon) Ontology" .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2004/02/skos/core#definition
<http://www.w3.org/2004/02/skos/core#definition> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
<http://www.w3.org/2004/02/skos/core#example> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#note
<http://www.w3.org/2004/02/skos/core#note> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.purl.org/graphemon#asciiTransliterationLiteral
:asciiTransliterationLiteral rdf:type rdfs:Datatype ;
                             rdfs:label "ASCII transliteration literal" ;
                             <http://www.w3.org/2004/02/skos/core#definition> "A literal type which describes transliterations of graphemes in ASCII characters only" .


###  http://www.purl.org/graphemon#cssLiteral
:cssLiteral rdf:type rdfs:Datatype ;
            rdfs:label "CSS Literal" ;
            <http://www.w3.org/2004/02/skos/core#definition> "A CSS literal which may be used to describe glyph properties" .


###  http://www.purl.org/graphemon#extendedGottsteinLiteral
:extendedGottsteinLiteral rdf:type rdfs:Datatype .


###  http://www.purl.org/graphemon#gottsteinLiteral
:gottsteinLiteral rdf:type rdfs:Datatype ;
                  rdfs:label "Gottstein Literal" ;
                  <http://www.w3.org/2004/02/skos/core#definition> "String literal type describing a cuneiform sign using its Gottstein code" .


###  http://www.purl.org/graphemon#paleocodeLiteral
:paleocodeLiteral rdf:type rdfs:Datatype ;
                  rdfs:label "PaleoCode Literal" ;
                  <http://www.w3.org/2004/02/skos/core#definition> "String literal describing a cuneiform sign using the PaleoCodage encoding" .


###  http://www.purl.org/graphemon#svgLiteral
:svgLiteral rdf:type rdfs:Datatype ;
            rdfs:label "SVG Literal" .


###  http://www.purl.org/graphemon#unicodeTransliterationLiteral
:unicodeTransliterationLiteral rdf:type rdfs:Datatype ;
                               rdfs:label "Unicode transliteration literal" .


#################################################################
#    Object Properties
#################################################################

###  http://lemon-model.net/lemon#reference
lemon:reference rdf:type owl:ObjectProperty ;
                rdfs:label "reference" .


###  http://www.cidoc-crm.org/cidoc-crm/P62_depicted_by
cidoc:P62_depicted_by rdf:type owl:ObjectProperty ;
                      rdfs:label "depicted by" .


###  http://www.purl.org/graphemon#above
:above rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :directionProperty ;
       rdfs:domain :GraphemeAtomicPart ;
       rdfs:range :GraphemeAtomicPart ;
       rdfs:label "above" ;
       <http://www.w3.org/2004/02/skos/core#definition> "Property indicating that an atomic part is above another atomic part" .


###  http://www.purl.org/graphemon#below
:below rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :directionProperty ;
       rdfs:domain :GraphemeAtomicPart ;
       rdfs:range :GraphemeAtomicPart ;
       rdfs:label "below" ;
       <http://www.w3.org/2004/02/skos/core#definition> "Property indicating that an atomic part is below another atomic part" .


###  http://www.purl.org/graphemon#directionProperty
:directionProperty rdf:type owl:ObjectProperty ;
                   rdfs:domain :GraphemeAtomicPart ;
                   rdfs:range :GraphemeAtomicPart ;
                   rdfs:label "direction property" ;
                   <http://www.w3.org/2004/02/skos/core#definition> "A property indicating the direction of a grapheme atomic part" .


###  http://www.purl.org/graphemon#glyphAspectProperty
:glyphAspectProperty rdf:type owl:ObjectProperty ;
                     rdfs:label "glyph aspect property" .


###  http://www.purl.org/graphemon#graphemeEtySource
:graphemeEtySource rdf:type owl:ObjectProperty ;
                   rdfs:domain :GraphemeEtylink ;
                   rdfs:range :GraphemeManifestation ;
                   rdfs:label "grapheme etymology source" .


###  http://www.purl.org/graphemon#graphemeEtyTarget
:graphemeEtyTarget rdf:type owl:ObjectProperty ;
                   rdfs:domain :GraphemeEtylink ;
                   rdfs:range :GraphemeManifestation ;
                   rdfs:label "grapheme etymology target" .


###  http://www.purl.org/graphemon#graphemeSense
:graphemeSense rdf:type owl:ObjectProperty ;
               rdfs:domain :GraphemeAtomicPart ,
                           :GraphemeManifestation ,
                           :GraphemePart ;
               rdfs:range :GraphemeSense ;
               rdfs:label "grapheme sense" ;
               <http://www.w3.org/2004/02/skos/core#definition> "Relates a sense instance to a given grapheme" .


###  http://www.purl.org/graphemon#hasAtomicPart
:hasAtomicPart rdf:type owl:ObjectProperty ;
               rdfs:domain cidoc:TXP9_Grapheme ,
                           :GraphemePart ,
                           :GraphemeVariant ;
               rdfs:range :GraphemeAtomicPart ;
               rdfs:label "has atomic part" ;
               <http://www.w3.org/2004/02/skos/core#definition> "Relates to an atomic part which is included in the current grapheme" .


###  http://www.purl.org/graphemon#hasColor
:hasColor rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :glyphAspectProperty ;
          rdfs:domain cidoc:TX6_Glyph ;
          rdfs:label "has color" ;
          <http://www.w3.org/2004/02/skos/core#definition> "Relates a glyph or a glyph part of a color" .


###  http://www.purl.org/graphemon#hasGraphemeReading
:hasGraphemeReading rdf:type owl:ObjectProperty ;
                    rdfs:domain cidoc:TXP9_Grapheme ,
                                :GraphemeComposition ;
                    rdfs:range :GraphemeReading ;
                    rdfs:label "has grapheme reading" .


###  http://www.purl.org/graphemon#hasMaterial
:hasMaterial rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :glyphAspectProperty ;
             rdfs:domain cidoc:TX6_Glyph ;
             rdfs:label "has material" ;
             <http://www.w3.org/2004/02/skos/core#definition> "Definition of the material which was used to create a glyph representation" .


###  http://www.purl.org/graphemon#hasMaterialAspect
:hasMaterialAspect rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :glyphAspectProperty ;
                   rdfs:label "has material aspect" ;
                   <http://www.w3.org/2004/02/skos/core#definition> "An aspect of the material of which the glyph is composed of" .


###  http://www.purl.org/graphemon#hasPart
:hasPart rdf:type owl:ObjectProperty ;
         rdfs:domain cidoc:TXP9_Grapheme ,
                     :GraphemeVariant ;
         rdfs:range :GraphemePart ;
         rdfs:label "has part" ;
         <http://www.w3.org/2004/02/skos/core#definition> "Part of a grapheme representation which may be reused in other graphemes" .


###  http://www.purl.org/graphemon#hasSource
:hasSource rdf:type owl:ObjectProperty ;
           rdfs:label "has source" ;
           <http://www.w3.org/2004/02/skos/core#definition> "Defines the source of an etymological relation" .


###  http://www.purl.org/graphemon#hasTarget
:hasTarget rdf:type owl:ObjectProperty ;
           rdfs:label "has target" ;
           <http://www.w3.org/2004/02/skos/core#definition> "Defines the target of an etymological relation" .


###  http://www.purl.org/graphemon#isComposedOf
:isComposedOf rdf:type owl:ObjectProperty ;
              rdfs:domain :GraphemeComposition ;
              rdfs:range cidoc:TXP9_Grapheme ;
              rdfs:label "is composed of" ;
              <http://www.w3.org/2004/02/skos/core#definition> "Indicates an element of a graph composition" .


###  http://www.purl.org/graphemon#isGraphemeCognateOf
:isGraphemeCognateOf rdf:type owl:ObjectProperty ;
                     rdfs:domain :GraphemeManifestation ;
                     rdfs:range :GraphemeManifestation ;
                     rdfs:label "is grapheme cognate of" ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Relates a grapheme to its cognate grapheme" .


###  http://www.purl.org/graphemon#leftOf
:leftOf rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :directionProperty ;
        rdfs:domain :GraphemeAtomicPart ;
        rdfs:range :GraphemeAtomicPart ;
        rdfs:label "left of" ;
        <http://www.w3.org/2004/02/skos/core#definition> "Property indicating that an atomic part is left of another atomic part" .


###  http://www.purl.org/graphemon#linkBasedOn
:linkBasedOn rdf:type owl:ObjectProperty ;
             rdfs:domain :SimilarityLink ;
             rdfs:range :SimilarityMetric ;
             rdfs:label "link based on" ;
             <http://www.w3.org/2004/02/skos/core#definition> "Metric or measurement a similarity link instance is based on" .


###  http://www.purl.org/graphemon#rightOf
:rightOf rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :directionProperty ;
         rdfs:domain :GraphemeAtomicPart ;
         rdfs:range :GraphemeAtomicPart ;
         rdfs:label "right of" ;
         <http://www.w3.org/2004/02/skos/core#definition> "Property indicating that an atomic part is right of another atomic part" .


###  http://www.purl.org/graphemon#similarityLinkSource
:similarityLinkSource rdf:type owl:ObjectProperty ;
                      rdfs:domain :GraphemeManifestation ;
                      rdfs:range :GraphemeManifestation ;
                      rdfs:label "similarity link source" ;
                      <http://www.w3.org/2004/02/skos/core#definition> "The source of a similarity link" .


###  http://www.purl.org/graphemon#similarityLinkTarget
:similarityLinkTarget rdf:type owl:ObjectProperty ;
                      rdfs:domain :GraphemeManifestation ;
                      rdfs:range :GraphemeManifestation ;
                      rdfs:label "similarity link target" ;
                      <http://www.w3.org/2004/02/skos/core#definition> "The target of a similarity link" .


###  http://www.purl.org/graphemon#timeperiod
:timeperiod rdf:type owl:ObjectProperty ;
            rdfs:label "time period" .


###  http://www.purl.org/graphemon#variant
:variant rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain cidoc:TXP9_Grapheme ;
         rdfs:range :GraphemeVariant ;
         rdfs:label "variant" ;
         <http://www.w3.org/2004/02/skos/core#definition> "Links a grapheme to its grapheme variant" .


#################################################################
#    Data properties
#################################################################

###  http://www.purl.org/graphemon#abzl
:abzl rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf :hasCuneiformIdentifier ;
      rdfs:domain cidoc:TXP9_Grapheme ,
                  :GraphemeManifestation ;
      rdfs:range xsd:integer ;
      rdfs:label "abzl" ;
      <http://www.w3.org/2004/02/skos/core#definition> "Relates to an identifier in the mesopotamian sign dictionary" .


###  http://www.purl.org/graphemon#altSignName
:altSignName rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :signName ;
             rdfs:label "An alternative designation describing an individual grapheme" ,
                        "alternative sign name" .


###  http://www.purl.org/graphemon#asPNG
:asPNG rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf :hasImage ;
       rdfs:range xsd:anyURI ;
       rdfs:label "as PNG" ;
       <http://www.w3.org/2004/02/skos/core#definition> "Defines an PNG representation of a grapheme instance" .


###  http://www.purl.org/graphemon#asSVG
:asSVG rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf :hasImage ;
       rdfs:domain cidoc:TXP9_Grapheme ,
                   :GraphemeAtomicPart ,
                   :GraphemePart ;
       rdfs:range :svgLiteral ,
                  xsd:anyURI ;
       rdfs:label "as SVG" ;
       <http://www.w3.org/2004/02/skos/core#definition> "Defines an SVG representation of a grapheme instance" .


###  http://www.purl.org/graphemon#hasCuneiformGraphemeDescription
:hasCuneiformGraphemeDescription rdf:type owl:DatatypeProperty ;
                                 rdfs:subPropertyOf :hasGraphemeDescription ;
                                 rdfs:label "has cuneiform grapheme description" ;
                                 <http://www.w3.org/2004/02/skos/core#definition> "A grapheme description for a cuneiform sign" .


###  http://www.purl.org/graphemon#hasCuneiformIdentifier
:hasCuneiformIdentifier rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf :hasIdentifier ;
                        rdfs:label "has cuneiform identifier" ;
                        <http://www.w3.org/2004/02/skos/core#definition> "An identifier which relates to a definition in a cuneiform dictionary or sign list" .


###  http://www.purl.org/graphemon#hasExtendedGottsteinCode
:hasExtendedGottsteinCode rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf :hasGottsteinCode ;
                          rdfs:domain :GraphemeVariant ;
                          rdfs:range :extendedGottsteinLiteral ;
                          rdfs:label "has extended Gottstein Code" ;
                          <http://www.w3.org/2004/02/skos/core#definition> "An extended Gottstein code description of a cuneiform sign" ;
                          <http://www.w3.org/2004/02/skos/core#note> "An extended Gottstein description exposes the Winkelhaken as an extra character w" .


###  http://www.purl.org/graphemon#hasGottsteinCode
:hasGottsteinCode rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :hasCuneiformGraphemeDescription ;
                  rdfs:domain :GraphemeVariant ;
                  rdfs:range :gottsteinLiteral ;
                  rdfs:label "has Gottstein Code" ;
                  <http://www.w3.org/2004/02/skos/core#definition> "A Gottstein code description of a cuneiform sign" .


###  http://www.purl.org/graphemon#hasGraphemeDescription
:hasGraphemeDescription rdf:type owl:DatatypeProperty ;
                        rdfs:label "has grapheme description" ;
                        <http://www.w3.org/2004/02/skos/core#definition> "A description of a grapheme in a define character descripton system" .


###  http://www.purl.org/graphemon#hasIdentifier
:hasIdentifier rdf:type owl:DatatypeProperty ;
               rdfs:domain cidoc:TXP9_Grapheme ,
                           :GraphemeVariant ;
               rdfs:range xsd:string ;
               rdfs:label "has identifier" ;
               <http://www.w3.org/2004/02/skos/core#definition> "Relates a Grapheme to a String representation identifiying it in a particular context" .


###  http://www.purl.org/graphemon#hasImage
:hasImage rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :hasMedia ;
          rdfs:range xsd:anyURI ;
          rdfs:label "has image" ;
          <http://www.w3.org/2004/02/skos/core#definition> "Relates a grapheme representation to a media representation in form of an image" .


###  http://www.purl.org/graphemon#hasMedia
:hasMedia rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:range xsd:anyURI ;
          rdfs:label "has media" ;
          <http://www.w3.org/2004/02/skos/core#definition> "Relates a grapheme manifestation to a media representation" .


###  http://www.purl.org/graphemon#hasPaleoCode
:hasPaleoCode rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :hasCuneiformGraphemeDescription ;
              rdfs:domain :GraphemeVariant ;
              rdfs:range :paleocodeLiteral ;
              rdfs:label "has PaleoCode" .


###  http://www.purl.org/graphemon#hasUnicodeCodePoint
:hasUnicodeCodePoint rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf :hasIdentifier ;
                     rdfs:domain cidoc:TXP9_Grapheme ,
                                 :GraphemeVariant ;
                     rdfs:range xsd:string ;
                     rdfs:label "has unicode code point" ;
                     <http://www.w3.org/2004/02/skos/core#definition> "Relates a grapheme to a unicode code point which identifies it" .


###  http://www.purl.org/graphemon#hasUnicodeRepresentation
:hasUnicodeRepresentation rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf :hasIdentifier ;
                          rdfs:domain :GraphemeManifestation ;
                          rdfs:range xsd:string ;
                          rdfs:label "has unicode repesentation" .


###  http://www.purl.org/graphemon#hasVideo
:hasVideo rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :hasMedia ;
          rdfs:range xsd:anyURI ;
          rdfs:label "has video" ;
          <http://www.w3.org/2004/02/skos/core#definition> "Relates a grapheme representation to a media representation in the form of a video" .


###  http://www.purl.org/graphemon#hethzl
:hethzl rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf :hasCuneiformIdentifier ;
        rdfs:label "hethzl" ;
        <http://www.w3.org/2004/02/skos/core#definition> "Relates to an identifier in the hittite sign dictionary" .


###  http://www.purl.org/graphemon#isCanonicalForm
:isCanonicalForm rdf:type owl:DatatypeProperty ;
                 rdfs:domain :GraphemeVariant ;
                 rdfs:range xsd:boolean ;
                 rdfs:label "is canonical form" ;
                 <http://www.w3.org/2004/02/skos/core#definition> "Indicates whether a grapheme variant is considered as a canonical form" .


###  http://www.purl.org/graphemon#isDamaged
:isDamaged rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:domain cidoc:TX6_Glyph ;
           rdfs:range xsd:boolean ;
           rdfs:label "is damaged" ;
           <http://www.w3.org/2004/02/skos/core#definition> "Indicates whether a glyph is damaged" .


###  http://www.purl.org/graphemon#isErased
:isErased rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:range xsd:boolean ;
          rdfs:label "is erased" ;
          <http://www.w3.org/2004/02/skos/core#definition> "Indicates whether a glyph representation is erased" .


###  http://www.purl.org/graphemon#meszl
:meszl rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf :hasCuneiformIdentifier ;
       rdfs:domain cidoc:TXP9_Grapheme ,
                   :GraphemeManifestation ;
       rdfs:range xsd:integer ;
       rdfs:label "meszl" ;
       <http://www.w3.org/2004/02/skos/core#definition> "Number in the dictionary Mesopotamisches Zeichenlexikon" .


###  http://www.purl.org/graphemon#readingValue
:readingValue rdf:type owl:DatatypeProperty ;
              rdfs:label "reading value" ;
              <http://www.w3.org/2004/02/skos/core#definition> "The value of a reading which has been assigned to a character" .


###  http://www.purl.org/graphemon#signName
:signName rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :hasCuneiformIdentifier ;
          rdfs:range xsd:string ;
          rdfs:label "sign name" ;
          <http://www.w3.org/2004/02/skos/core#definition> "A designation describing an individual grapheme" .


###  http://www.purl.org/graphemon#similarityLowerBound
:similarityLowerBound rdf:type owl:DatatypeProperty ;
                      rdfs:range xsd:double ;
                      rdfs:label "similarity low bound" ;
                      <http://www.w3.org/2004/02/skos/core#definition> "The lower bound of a similarity measurement" .


###  http://www.purl.org/graphemon#similarityUpperBound
:similarityUpperBound rdf:type owl:DatatypeProperty ;
                      rdfs:label "similarity upper bound" ;
                      <http://www.w3.org/2004/02/skos/core#definition> "The upper bound of a similarity measurement" .


###  http://www.purl.org/graphemon#slha
:slha rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf :hasCuneiformIdentifier ;
      rdfs:label "slha" ;
      <http://www.w3.org/2004/02/skos/core#definition> "Deimel number of the Sumerian lexicon (SL) or number from the Handbuch Assur (HA)" .


###  http://www.purl.org/graphemon#upperBoundMostSimilar
:upperBoundMostSimilar rdf:type owl:DatatypeProperty ;
                       rdfs:range xsd:boolean ;
                       rdfs:label "upper bound most similar" .


#################################################################
#    Classes
#################################################################

###  http://lemon-model.net/lemon#LexialEntry
lemon:LexialEntry rdf:type owl:Class ;
                  rdfs:label "lexical entry" .


###  http://lemon-model.net/lemon#LexicalForm
lemon:LexicalForm rdf:type owl:Class ;
                  rdfs:label "lexical form" .


###  http://lemon-model.net/lemon#LexicalSense
lemon:LexicalSense rdf:type owl:Class ;
                   rdfs:label "sense" .


###  http://lemon-model.net/lemon#Lexicon
lemon:Lexicon rdf:type owl:Class ;
              rdfs:label "lexicon" .


###  http://lemon-model.net/lemon#Word
lemon:Word rdf:type owl:Class ;
           rdfs:subClassOf lemon:LexicalForm ;
           rdfs:label "word form" ;
           <http://www.w3.org/2004/02/skos/core#definition> "A grammatical form of a word" .


###  http://www.cidoc-crm.org/cidoc-crm/TX6_Glyph
cidoc:TX6_Glyph rdf:type owl:Class ;
                rdfs:label "glyph" ;
                <http://www.w3.org/2004/02/skos/core#definition> "The physical manifestation of a grapheme" .


###  http://www.cidoc-crm.org/cidoc-crm/TXP9_Grapheme
cidoc:TXP9_Grapheme rdf:type owl:Class ;
                    rdfs:subClassOf :GraphemeManifestation ;
                    rdfs:label "grapheme" ;
                    <http://www.w3.org/2004/02/skos/core#definition> "A standardized form of representation of a grapheme" .


###  http://www.purl.org/graphemon#ChineseCharacterStroke
:ChineseCharacterStroke rdf:type owl:Class ;
                        rdfs:subClassOf :Stroketyped ;
                        rdfs:label "Chinese character stroke" ;
                        <http://www.w3.org/2004/02/skos/core#definition> "Strokes used to draw Chinese characters" .


###  http://www.purl.org/graphemon#CuneiformWedge
:CuneiformWedge rdf:type owl:Class ;
                rdfs:subClassOf :Stroketyped ;
                rdfs:label "cuneiform wedge" ;
                <http://www.w3.org/2004/02/skos/core#definition> "Wedges used to write cuneiform signs" .


###  http://www.purl.org/graphemon#EgyptianHieroglyphicAtomicPart
:EgyptianHieroglyphicAtomicPart rdf:type owl:Class ;
                                rdfs:subClassOf :ideographic-typed ;
                                rdfs:label "egyptian hieroglyphic atomic part" ;
                                <http://www.w3.org/2004/02/skos/core#definition> "An undividable part of an egyptian hieroglyphic which is used to compose other hieroglyphics" .


###  http://www.purl.org/graphemon#GlyphCollection
:GlyphCollection rdf:type owl:Class ;
                 rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#Collection> ;
                 rdfs:label "glyph collection" ;
                 <http://www.w3.org/2004/02/skos/core#definition> "A collection of glyphs" .


###  http://www.purl.org/graphemon#GottsteinSystem
:GottsteinSystem rdf:type owl:Class ;
                 rdfs:subClassOf :GraphemeDescriptionSystem ;
